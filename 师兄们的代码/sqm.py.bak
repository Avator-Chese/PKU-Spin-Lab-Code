        print COLUMNS
        
        if not os.path.exists(pathfile):
            f1=open(pathfile, 'a')
            f1.write(COLUMNS)
            f1.close()

    def fmr_measure(self, pathfile, field, field_rate):
        field_end=0
        global DATA
        self.fmr_text_init(pathfile)
        ppms=PPMS()
        ppms.setField(field)
        ppms.setField(field_end,field_rate,stable=0)
