            if not DATA==data_last:
                f1=open(pathfile, 'a')
                f1.write(DATA)
                f1.close()
                data_last=DATA
                print field_realtime
                time.sleep(0.5)
        
        if field_end !=0:
            ppms.setField(0,field_rate,stable=0)
        
                

