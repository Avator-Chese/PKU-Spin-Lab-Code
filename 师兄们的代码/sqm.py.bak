        return line
    
    def get_freq(self):
        return self.pna.query(':CALC1:MEAS1:X?') 
    
    
    def fmr_init(self,measure_type='S21',freq_start=500e3,freq_stop=43e9,points=1601,power=-25,ave=16,IF=10e3,sweep_time=0):
