    def set_I_HIGH(self,I_HIGH=1e-3):
        self.k6221.write('SOUR:DELT:HIGH {}'.format(str(I_HIGH)))
    def set_I_LOW(self,I_LOW=-1e-3):
        self.k6221.write('SOUR:DELT:LOW '+ str(I_LOW))
